    "extract",
    "iterator",
    "asyncIterator",
    "Symbol.asyncIterator",
    "extract-zip",
    "decompress",
    "decompress-zip",
    ".zip",
    "zip",
    "yauzl",
    "zip-stream",
    "stream"
  ],
  "homepage": "https://github.com/kmalakoff/zip-iterator",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/zip-iterator.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts"
  },
  "dependencies": {
    "end-of-stream": "^1.4.4",
    "extract-base-iterator": "^0.2.5",
    "fs-access-compat": "^1.0.2",
    "inherits": "^2.0.4",
